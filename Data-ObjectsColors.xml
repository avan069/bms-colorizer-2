<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Resources>

	<!-- Objects Colors -->

	<!--
		Color codes are:

			#RRGGBB

		Except for threats (SAM/AAA domes) which are:

			(hue, saturation, lightness)
			#HHSSLL

		Note that lightness is currently ignored by Tacview.
	-->

	<!-- Colors used for very specific cases -->

	<SpecialColors>

		<!-- Default Color -->
		<!-- Used only when the object is completely unknown (not even recognized in the database) -->

		<Color ID="Default">
			<Icon>#606060</Icon>
			<Top>#d9d9d9</Top>
			<Bottom>#8E8E8E</Bottom>
			<Side>#ADADAD</Side>
			<Lines>#ADADAD</Lines>
			<Threat>#0000d8</Threat>
		</Color>

		<!-- Destroyed objects -->
		<!-- Ground objects which are destroyed remains displayed, but without their label and using this color. -->

		<Color ID="Destroyed">
			<Icon>#808080</Icon>
			<Top>#828282</Top>
			<Bottom>#333333</Bottom>
			<Side>#5E5E5E</Side>
			<Lines>#5E5E5E</Lines>
			<Threat>#0000d8</Threat>
		</Color>

	</SpecialColors>

